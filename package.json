{
  "name": "eslint-watcher",
  "version": "1.2.2-beta.4",
  "description": "Watches selected files in your repo for new saves and runs ESLint in its own terminal when it sees a save.",
  "main": "scripts/eslint-watcher.js",
  "scripts": {
    "postinstall": "node -e \"require('child_process').execSync('node scripts/setup.js', { stdio: 'inherit' })\"",
    "setup": "node scripts/setup.js",
    "eslint-watcher": "node scripts/eslint-watcher.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdemelle-SiP/eslint-watcher.git"
  },
  "keywords": [
    "ESLint",
    "automated",
    "watcher",
    "save"
  ],
  "author": "Todd DeMelle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tdemelle-SiP/eslint-watcher/issues"
  },
  "homepage": "https://github.com/tdemelle-SiP/eslint-watcher#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.56.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.29.4"
  }
}
